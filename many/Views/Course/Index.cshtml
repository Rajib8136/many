@model IEnumerable<many.Models.Course>
@{ 
    ViewData["title"] = "Index";
}
<h1> Index </h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(c => item.Name)</td>
                </tr>
            }
    </tbody>
</table>