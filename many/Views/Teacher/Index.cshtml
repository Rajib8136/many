@model IEnumerable<many.Models.Teacher>
@{ 
    ViewData["Title"] = "Index ";
}

<h1> Index Teacher</h1>
<a asp-action="Create"> Create Here</a>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(Model => Model.Name)</th>
            <th>@Html.DisplayNameFor(model => model.Address)</th>
            <th>@Html.DisplayNameFor(Model => Model.Faculty)</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(model => item.Name)</td>
                    <td>@Html.DisplayFor(model => item.Address)</td>
                    <td>@Html.DisplayFor(model=> item.Faculty)</td>
                </tr>
            }
    </tbody>
</table>